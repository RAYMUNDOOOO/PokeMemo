<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="clr-namespace:PokeMemo.ViewModels"
			 xmlns:models="clr-namespace:PokeMemo.Models"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PokeMemo.Views.ModifyDeckView"
			 x:CompileBindings="False">
	<Design.DataContext>
		<local:ModifyDeckViewModel/>
	</Design.DataContext>
	<StackPanel>
		<Button Content="Back to Deck Library" Command="{Binding NavigateToDeckLibraryViewCommand}" HorizontalAlignment="Right" VerticalAlignment="Top" Padding="40" Margin="10"/>
		<TextBlock Text="This is the Modify Deck View" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center"/>
	</StackPanel>
</UserControl>

